{
  "name": "mml-api",
  "version": "3.0.0",
  "description": "api server for mml",
  "main": "server.js",
  "scripts": {
    "devStart": "node server.js",
    "pkg": "pkg server.js --target node18-win-x64 --output server.exe"
  },
  "author": "Joshua Powell",
  "license": "MIT",
  "dependencies": {
    "archiver": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "inquirer": "^9.2.15",
    "mml-api": "file:",
    "mongoose": "^8.1.1",
    "multer": "^1.4.5-lts.1",
    "node-windows": "^1.0.0-beta.8",
    "readline": "^1.3.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.2"
  },
  "pkg": {
    "scripts": [
      "*.js",
      "routers/*.js",
      "models/*.js",
      "helpers/*.js",
      "middleware/*.js"
    ],
    "assets": [
      "uploads/**/*",
      "build/**/*",
      "logs/*/*",
      "config.json",
      "apiToken.json"
    ]
  }
}
